  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint-config-prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react-virtualized": "^9.21.30",
    "eslint-config-prettier": "^10.1.8",
    "msw": "^2.10.4",
    "prettier": "^3.6.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@reduxjs/toolkit|@standard-schema|date-fns|@mui/x-date-pickers|react-router|react-router-dom|framer-motion|cmdk|react-use))"
    ]
  }
}
