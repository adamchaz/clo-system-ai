    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.4.3",
    "@types/jest": "^27.5.2",
    "@types/numeral": "^2.0.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-redux": "^7.1.34",
    "axios": "^1.12.0",
    "cmdk": "^0.2.0",
    "d3": "^7.9.0",
